<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/GameService.Enums" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/GameService.Enums" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="Player">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" />
      <xs:enumeration value="One" />
      <xs:enumeration value="Two" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="Player" nillable="true" type="tns:Player" />
  <xs:simpleType name="GameState">
    <xs:restriction base="xs:string">
      <xs:enumeration value="WaitingPlayer" />
      <xs:enumeration value="Running" />
      <xs:enumeration value="Finished" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="GameState" nillable="true" type="tns:GameState" />
</xs:schema>